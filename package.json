{
  "name": "shopify-koa-app",
  "version": "1.0.0",
  "description": "An embedded Shopify app!",
  "main": "index.ts",
  "scripts": {
    "dev": "nodemon --watch client --watch server --exec babel-node index.ts --extensions \".js,.ts,.tsx\"",
    "postinstall": "npm run build-babel && npm run build",
    "start": "npm run server",
    "server": "cross-env NODE_ENV=production node ./dist/index.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress --colors",
    "build-babel": "npm run clean && babel -d ./dist/server ./server -s --extensions \".js,.ts,.tsx\" && babel -d ./dist index.ts --extensions \".js,.ts,.tsx\" && babel -d ./dist/client ./client && babel -d ./dist webpack.config.js && babel -d ./dist/server/views ./server/views --copy-files",
    "clean": "rm -rf ./dist && mkdir ./dist",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "engines": {
    "node": "10.15.3"
  },
  "author": "spchuang",
  "license": "MIT",
  "dependencies": {
    "@shopify/koa-shopify-auth": "^3.1.46",
    "@shopify/koa-shopify-graphql-proxy": "^3.2.1",
    "@shopify/polaris": "4.6.1",
    "@shopify/react-effect": "^3.2.5",
    "@shopify/react-html": "9.2.5",
    "@shopify/react-shopify-app-route-propagator": "^3.0.3",
    "cross-env": "^5.2.0",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "ejs": "^2.6.1",
    "graphql": "^14.0.2",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.10.0",
    "koa-bodyparser": "^4.2.1",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-session": "^5.12.3",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.1",
    "koa-webpack": "^5.2.4",
    "lodash": "^4.17.15",
    "nullthrows": "^1.1.1",
    "path": "^0.12.7",
    "react": "^16.6.1",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.6.1",
    "react-fetch-component": "^7.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "shopify-api-node": "^2.23.1",
    "webpack": "^4.25.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/dotenv": "^6.1.1",
    "@types/invariant": "^2.2.30",
    "@types/koa": "^2.0.49",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-cors": "0.0.0",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-router": "^7.0.42",
    "@types/koa-session": "^5.10.1",
    "@types/koa-static": "^4.0.1",
    "@types/koa-views": "^2.0.3",
    "@types/koa-webpack": "^5.0.1",
    "@types/lodash": "^4.14.144",
    "@types/node": "^12.11.1",
    "@types/webpack": "^4.39.1",
    "@types/webpack-env": "^1.14.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-styled-components": "^1.8.0",
    "css-loader": "^3.2.0",
    "kill-port": "^1.3.2",
    "mini-css-extract-plugin": "^0.4.5",
    "nodemon": "^1.19.4",
    "prettier": "^1.15.1",
    "style-loader": "^0.23.1",
    "typescript": "^3.6.2",
    "webpack-cli": "^3.1.2"
  }
}
